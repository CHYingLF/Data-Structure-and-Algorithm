    "#helix\n",
    "t=turtle.Turtle()\n",
    "def drawSpiral(t,lineLen):\n",
    "    if lineLen>0:\n",
    "        t.forward(lineLen)\n",
    "        t.right(90)\n",
    "        drawSpiral(t,lineLen-5)\n",
    "\n",
    "drawSpiral(t,100)\n",
    "turtle.done"
   ]
